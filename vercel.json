{
    "version": 2,
    "builds": [
      {
        "src": "./backend/index.js",  
        "use": "@vercel/node"
      },
      {
        "src": "./client/build",   
        "use": "@vercel/static"
      }
    ],
  
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/backend/index.js"
      }
    ]
  }